{
    "$schema": "http://json-schema.org/draft-07/schema",
    "$id": "http://github.com/tjurczyk/arkadia-data/magics_data.schema.json",
    "type": "object",
    "title": "The root schema",
    "description": "Magics schema.",
    "default": {},
    "examples": [
        {
            "version": 2,
            "magics": {
                "adamantytowa szpiczasta partyzana": {
                    "type": [
                        "dwureczna bron drzewcowa"
                    ],
                    "regexps": [
                        "adamantytowa szpiczasta partyzana",
                        "adamantytowa szpiczasta partyzane",
                        "adamantytowe szpiczaste partyzany",
                        "adamantytowej szpiczastej partyzanie",
                        "adamantytowej szpiczastej partyzany"
                    ]
                }
            }
        }
    ],
    "required": [
        "version",
        "magics"
    ],
    "properties": {
        "version": {
            "$id": "#/properties/version",
            "type": "integer",
            "title": "The version schema",
            "description": "An explanation about the purpose of this instance.",
            "default": 0,
            "examples": [
                2
            ]
        },
        "magics": {
            "default": {},
            "required": [],
            "title": "The magics schema",
            "additionalProperties": true
        }
    },
    "additionalProperties": true
}